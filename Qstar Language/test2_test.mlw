module Main

use export binary.Bit_vector
use wired_circuits.Circuit_c
use export p_int.Int_comp
use ref.Ref

let main (n:int): circuit
    requires {n>0}
    ensures {true}
    =
    begin
        let c' = ref (m_skip n)
        in
        c':= !c' -- (place hadamard 0 n);
        assert {true};
        c':= !c' -- (place xx 0 n);
        assert {true};
        return !c'
    end
end
